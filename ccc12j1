#include <bits/stdc++.h>
using namespace std;
int main() {
    int n, m;
    cin >> n >> m;
    if (m <= n) {
        cout << "Congratulations, you are within the speed limit!";
    } else if (m - n < 21) {
        cout << "You are speeding and your fine is $100.";
    } else if (m - n < 31) {
        cout << "You are speeding and your fine is $270.";
    } else {
        cout << "You are speeding and your fine is $500.";
    }
    return 0;
}
