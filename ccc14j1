#include <bits/stdc++.h>
using namespace std;
int main() {
    int a, b, c;
    cin >> a >> b >> c;
    if (a + b + c != 180) {
        cout << "Error";
    } else if (a == b && b == c && c == 60) {
        cout << "Equilateral";
    } else if (a == b || b == c || a == c) {
        cout << "Isosceles";
    } else {
        cout << "Scalene";
    }
    return 0;
}
