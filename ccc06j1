#include <bits/stdc++.h>
using namespace std;
int a[4] = {461, 431, 420, 0}, b[4] = {100, 57, 70, 0}, c[4] = {130, 160, 118, 0}, d[4] = {167, 266, 75, 0};
int main() {
    int n, m, k, x;
    cin >> n >> m >> k >> x;
    cout << "Your total Calorie count is " << a[n - 1] + b[m - 1] + c[k - 1] + d[x - 1] << ".";
    return 0;
}
